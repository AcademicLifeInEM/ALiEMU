{
    "name": "aliemu.com",
    "version": "2.1.3",
    "license": "MIT",
    "repository": "https://github.com/aliemteam/ALiEMU",
    "scripts": {
        "lint:scss": "stylelint \"src/**/*.scss\"",
        "lint:ts": "tslint -p . -t stylish",
        "lint:php": "npm run phpcs -- --ignore=src/learndash --report=${PHPCS_REPORT:-summary} --basepath=. ./src",
        "phpcs": "[[ -z $CI ]] || docker run -it --rm -v $(pwd):/app dsifford/phpcs",
        "lint": "npm run lint:scss && npm run lint:ts && npm run lint:php",
        "pretest": "PHPCS_REPORT=full npm run lint",
        "sync": "./lib/sync.sh",
        "test": "jest --coverage ./src",
        "view-coverage": "$(test $(uname) == Linux && echo xdg-open || echo open) ./node_modules/.cache/coverage/lcov-report/index.html",
        "up": "docker-compose up -d && docker-compose logs -f wordpress",
        "down": "docker-compose down -v",
        "build": "TS_NODE_PROJECT=./tsconfig.node.json TS_NODE_FILES=true webpack --mode production",
        "build:dev": "TS_NODE_PROJECT=./tsconfig.node.json TS_NODE_FILES=true webpack --mode development",
        "start": "npm run build:dev -- --watch"
    },
    "dependencies": {
        "@babel/runtime": "^7.4.3",
        "classnames": "^2.2.6",
        "datalist-polyfill": "^1.23.3",
        "date-fns": "^1.30.1",
        "mobx": "^5.9.4",
        "mobx-react": "^5.4.3",
        "mobx-utils": "^5.2.0",
        "normalize.css": "^8.0.1",
        "react": "^16.8.6",
        "react-content-loader": "^4.2.1",
        "react-dom": "^16.8.6",
        "react-spring": "^8.0.19",
        "striptags": "^3.1.1",
        "typescript": "^3.4.2"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/plugin-transform-runtime": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-react": "^7.0.0",
        "@dsifford/stylelint-config": "^0.2.0",
        "@svgr/webpack": "^4.1.0",
        "@types/classnames": "^2.2.7",
        "@types/copy-webpack-plugin": "^4.4.3",
        "@types/dotenv": "^6.1.1",
        "@types/enzyme": "^3.9.1",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/googlemaps": "^3.30.19",
        "@types/grecaptcha": "^2.0.36",
        "@types/imagemin": "^6.0.0",
        "@types/imagemin-svgo": "^7.0.0",
        "@types/jest": "^24.0.11",
        "@types/jquery": "^3.3.29",
        "@types/mini-css-extract-plugin": "^0.2.0",
        "@types/node": "^11.13.2",
        "@types/react": "^16.8.13",
        "@types/react-addons-test-utils": "^0.14.24",
        "@types/react-content-loader": "^3.1.4",
        "@types/react-dom": "^16.8.3",
        "@types/react-test-renderer": "^16.8.1",
        "@types/rimraf": "^2.0.2",
        "@types/webpack": "^4.4.27",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.5",
        "browser-sync": "^2.26.3",
        "browser-sync-webpack-plugin": "^2.2.2",
        "copy-webpack-plugin": "^5.0.2",
        "css-loader": "^2.1.1",
        "cssnano": "^4.1.10",
        "dotenv": "^7.0.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.12.1",
        "identity-obj-proxy": "^3.0.0",
        "imagemin": "^6.1.0",
        "imagemin-pngquant": "^7.0.0",
        "imagemin-svgo": "^7.0.0",
        "jest": "^24.7.1",
        "mini-css-extract-plugin": "^0.5.0",
        "mobx-react-devtools": "^6.1.1",
        "node-sass": "^4.11.0",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.6.0",
        "prettier": "^1.16.4",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "^16.8.6",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "snapshot-diff": "^0.5.1",
        "style-loader": "^0.23.1",
        "stylelint": "^9.10.1",
        "stylelint-scss": "^3.5.4",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.0.3",
        "tsconfig-paths": "^3.8.0",
        "tslint": "^5.15.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-microsoft-contrib": "^6.1.0",
        "tslint-react": "^4.0.0",
        "typescript-tslint-plugin": "^0.3.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-fix-style-only-entries": "^0.2.1"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!**/{vendor,__tests__}/**"
        ],
        "coverageDirectory": "node_modules/.cache/coverage",
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.node.json"
            }
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy",
            "^(utils|components)/(.*)$": "<rootDir>/src/js/$1/$2"
        },
        "modulePathIgnorePatterns": [
            "<rootDir>/wp-content/"
        ],
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "testURL": "http://localhost",
        "transform": {
            ".(ts|tsx)": "ts-jest"
        }
    }
}
