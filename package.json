{
  "name": "aliemu",
  "version": "1.0.0",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "scripts": {
    "start": "gulp serve",
    "chown": "sudo chown -R $USER dist",
    "test": "jest"
  },
  "dependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.10.0",
    "coveralls": "^2.11.9",
    "cssnano": "^3.5.2",
    "del": "^2.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "jest-cli": "^12.0.2",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.0.1",
    "sugarss": "^0.1.3",
    "ts-loader": "^0.8.1",
    "typescript": "^1.9.0-dev.20160412",
    "webpack": "^1.12.14",
    "webpack-stream": "^3.1.0"
  },
  "jest": {
    "scriptPreprocessor": "jestPreprocessor.js",
    "testFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "verbose": true,
    "collectCoverage": true
  }
