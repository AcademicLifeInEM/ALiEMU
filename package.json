{
  "name": "aliemu",
  "version": "1.0.0",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "license": "MIT",
  "repository": "https://github.com/AcademicLifeInEM/ALiEMU",
  "scripts": {
    "lint": "npm run lint:stylint && npm run lint:tslint && npm run lint:eslint",
    "lint:stylint": "stylint aliemu/styles -c .stylintrc",
    "lint:tslint": "tslint -e \"node_modules/**/*\" \"**/*.+(ts|tsx)\" -t stylish",
    "lint:eslint": "eslint \"aliemu/**/*.js\" \"lib/**/*.js\" \"./*.js\" --ignore-pattern \"aliemu/vendor/*\"",
    "build": "NODE_ENV=production node_modules/.bin/gulp _build",
    "start": "NODE_ENV=development node --max_old_space_size=8192 node_modules/.bin/gulp _dev",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@types/enzyme": "^2.5.37",
    "@types/jest": "^16.0.1",
    "@types/moment": "^2.13.0",
    "@types/react": "^0.14.54",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-datepicker": "^0.28.6",
    "@types/react-dom": "^0.14.18",
    "@types/sinon": "^1.16.31",
    "mobx": "^2.6.5",
    "mobx-react": "^4.0.3",
    "moment": "^2.17.1",
    "react": "^15.3.2",
    "react-datepicker": "^0.37.0",
    "react-dom": "^15.3.2",
    "typescript": "^2.1.0-dev.20161109"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "awesome-typescript-loader": "^3.0.0-beta.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "browser-sync": "^2.17.5",
    "coveralls": "^2.11.14",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-csso": "^2.0.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-insert": "^0.5.0",
    "gulp-jade2php": "^1.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^2.0.0",
    "jest-cli": "^17.0.0",
    "merge-stream": "^1.0.0",
    "mobx-react-devtools": "^4.2.9",
    "poststylus": "^0.2.3",
    "react-addons-test-utils": "^15.3.2",
    "rucksack-css": "^0.9.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.7",
    "ts-loader": "^1.3.0",
    "tslint": "^4.0.2",
    "tslint-microsoft-contrib": "Microsoft/tslint-microsoft-contrib#releases",
    "tslint-react": "^2.0.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-bundle-analyzer": "^2.1.1",
    "webpack-stream": "^3.2.0"
  },
  "jest": {
    "transform": {
      ".*": "./lib/utils/jestPreprocessor.js"
    },
    "testRegex": "__tests__/.*\\.(js|ts|tsx)$",
    "automock": false,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "verbose": true,
    "coverageDirectory": "tmp/coverage",
    "collectCoverageFrom": [
      "aliemu/**/*.{ts,tsx}",
      "!aliemu/**/index.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ]
  }
}
