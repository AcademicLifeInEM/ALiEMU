{
  "name": "aliemu",
  "version": "1.0.0",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "license": "MIT",
  "repository": "https://github.com/AcademicLifeInEM/ALiEMU",
  "scripts": {
    "start": "gulp serve",
    "wp": "wpcmd() { docker exec $(docker ps -lq) /bin/bash -c \"sudo -u www-data wp $(echo $@)\"; };wpcmd ",
    "chown": "sudo chown -R $USER $(pwd)/dist",
    "pretest": "npm run lint",
    "test": "jest",
    "lint": "stylint wp-content/plugins/aliemu-plugins/inc/styles -c .stylintrc && tslint 'wp-content/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.12.7",
    "coveralls": "^2.11.9",
    "del": "^2.2.0",
    "enzyme": "^2.3.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.3.1",
    "gulp-uglify": "^1.5.1",
    "jest-cli": "^12.0.2",
    "poststylus": "^0.2.3",
    "react-addons-test-utils": "^15.0.1",
    "rucksack-css": "^0.8.6",
    "sinon": "^1.17.4",
    "stylint": "^1.3.10",
    "ts-loader": "^0.8.1",
    "tslint": "^3.10.1",
    "typescript": "^1.9.0-dev.20160412",
    "webpack": "^1.12.14",
    "webpack-stream": "^3.1.0"
  },
  "jest": {
    "scriptPreprocessor": "jestPreprocessor.js",
    "testFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme",
      "sinon"
    ],
    "verbose": true,
    "collectCoverage": true
  }
}
