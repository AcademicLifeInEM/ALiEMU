{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "*/**/*.ts",
        "*/**/*.tsx",
        "*/**/*.d.ts",
        "!node_modules/**",
        "!dist/**",
        "!typings/browser.d.ts",
        "!typings/browser/**"
