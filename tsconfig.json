    "compileOnSave": false,
    "buildOnSave": false,
    "files": [
        "typings/ALiEMU.d.ts",
        "typings/main.d.ts",
        "typings/main/ambient/enzyme/index.d.ts",
        "typings/main/ambient/jest/index.d.ts",
        "typings/main/ambient/react-addons-test-utils/index.d.ts",
        "typings/main/ambient/react-dom/index.d.ts",
        "typings/main/ambient/react/index.d.ts",
        "typings/main/ambient/require/index.d.ts",
        "typings/main/ambient/sinon/index.d.ts",
        "wp-content/plugins/aliemu-plugins/inc/utils/__tests__/BrowserDetect-test.ts",
        "wp-content/plugins/aliemu-plugins/inc/utils/__tests__/DashboardUtils-test.ts",
        "wp-content/plugins/aliemu-plugins/inc/utils/__tests__/Pagination-test.ts",
        "wp-content/plugins/aliemu-plugins/inc/utils/BrowserDetect.ts",
        "wp-content/plugins/aliemu-plugins/inc/utils/DashboardUtils.ts",
        "wp-content/plugins/aliemu-plugins/inc/utils/Mocks.ts",
        "wp-content/plugins/aliemu-plugins/inc/utils/Pagination.ts",
        "wp-content/plugins/aliemu-plugins/inc/components/TableComponents.tsx",
        "wp-content/plugins/aliemu-plugins/inc/dashboards/educator-dashboard/components/CourseTable.tsx",
        "wp-content/plugins/aliemu-plugins/inc/dashboards/educator-dashboard/components/StudentTable.tsx",
        "wp-content/plugins/aliemu-plugins/inc/dashboards/educator-dashboard/EducatorDashboard.tsx"
