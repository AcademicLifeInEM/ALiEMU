// Educator Dashboard
@media only screen and (max-width: 760px)
	// Force table to not be like tables anymore
	#au-enrolled-students-table table,
	#au-enrolled-students-table tbody,
	#au-enrolled-students-table td,
	#au-enrolled-students-table th,
	#au-enrolled-students-table thead,
	#au-enrolled-students-table tr
		display: block


	// Hide table headers (but not display: none, for accessibility)
	#au-enrolled-students-table thead tr
		position: absolute
		top: -9999px
		left: -9999px

	#au-enrolled-students-table tr
		border: 0 solid #ccc
		border-bottom: 10px solid gray

	#au-enrolled-students-table td
		// Behave  like a "row"
		border: none
		border-bottom: 1px solid #eee
		position: relative
		padding-left: 50%
		text-align: center

	#au-enrolled-students-table td:nth-of-type(1)
		color: black
		font-weight: 600

	#au-enrolled-students-table td:before
		// Now like a table header
		position: absolute
		// Top/left values mimic padding
		top: 6px
		left: 6px
		width: 45%
		padding-right: 10px
		white-space: nowrap
		text-align: left
		font-weight: 600
		color: #555

	/*
	Label the data
	*/
	#au-enrolled-students-table td:nth-of-type(1):before
		content: "Full Name:"

	#au-enrolled-students-table td:nth-of-type(2):before
		content: "Class:"

	#au-enrolled-students-table td:nth-of-type(3):before
		content: "Total III Hours:"

	#au-enrolled-students-table td:nth-of-type(4):before
		content: "User Export:"





// Headers / Footers
#et-main-area
	min-height: calc(100vh - 80px)


#main-content
	min-height: calc(100vh - 80px - 53px)


@media only screen and (min-width: 980px)
	#logo
		max-height: 30px

	header,
	header div,
	header div div
		height: 50px !important
		padding-top: 0 !important
		padding-bottom: 0 !important

	header div div a
		padding-bottom: 0 !important
		line-height: 50px

	#et_top_search span:before
		padding-top: 18px !important
		margin-top: 0 !important

	#et_top_search
		margin-right: 10px



@media only screen and (max-width: 980px)
	#logo
		max-height: 30px

	header,
	header div,
	header div div
		height: 50px !important
		padding: 0 !important

	#et-top-navigation .mobile_menu_bar
		padding: 8px 0 10px !important

	#et_search_icon:before
		margin-top: 9px

	#mobile_menu .sub-menu li
		display: none



// Mobile nav
.et_mobile_menu
	padding: 0
	width: 300px
	right: 0
	left: auto



// Course pages
.course-list-container
	display: flex
	flex-wrap: wrap
	justify-content: center
