		&.category-air
			background-color: #D0021B




	.au-coursebody
		padding: 5%
		height: 335px
		display: flex
		justify-content: flex-start
		flex-direction: column

		.coursebox-flex
			display: flex
			flex: auto
			flex-direction: column
			justify-content: center

			&:first-of-type
				justify-content: center
				max-height: 2em
				min-height: 2em


			&:nth-of-type(2)
				justify-content: flex-start


			&:last-of-type
				justify-content: flex-end
				align-items: flex-end


			h3
				font-weight: 700
				font-size: 1em
				color: #9B9B9B
				text-transform: uppercase
				padding: 0


			h1
				font-weight: 400
				background: none
				color: #191919
				padding: 0


			p
				font-weight: 300
				color: #666


			a
				padding: 10px 15px
				border: solid 2px
				border-radius: 5px
				text-decoration: none
				width: 120px

				&:hover
					color: white
					transition: 0.5s





